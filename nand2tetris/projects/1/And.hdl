/**
 * And gate:
 * if (a and b) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    Nand(a=a, b=b, out=NandAB);
    Not(in=NandAB, out=out);
}

/**
* And, in the end of the day, is just the negation of Nand.
* Where Nand is 0, and will be 1, and vice versa.

BarakXYZ 2024
*/
